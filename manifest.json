{
    "manifest_version": 2,
    "name": "UserSelecter",
    "description": "Extension for removing selection lock on websites",
    "version": "1.0",
    "browser_action": {
        "default_icon": "favicon.png",
        "default_popup": "popup.html",
        "default_title": "UserSelecter"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "all_frames": true,
        "js":      ["scripts/content.js"]
    }],
    "permissions": [
        "activeTab"
    ]
}

